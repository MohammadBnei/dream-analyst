    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@tailwindcss/postcss": "^4.1.16",
    "@tailwindcss/vite": "^4.1.16",
    "@types/node": "^24.9.1",
    "autoprefixer": "^10.4.21",
    "daisyui": "^5.3.9",
    "postcss": "^8.5.6",
    "release-it": "^19.0.5",
    "svelte": "^5.41.4",
    "svelte-check": "^4.3.3",
    "svelte-speech-recognition": "^0.0.7",
    "svelte-streamdown": "^2.6.1",
    "tailwindcss": "^4.1.16",
    "turbo": "^2.5.8",
    "typescript": "^5.9.3",
    "vite": "^7.1.12"
  },
  "type": "module",
  "dependencies": {
    "bcrypt": "^6.0.0",
    "dotenv": "^17.2.3",
    "jsonwebtoken": "^9.0.2",
    "ms": "^2.1.3",
    "postgres": "^3.4.7",
    "ws": "^8.18.3"
  },
  "release-it": {
    "$schema": "https://unpkg.com/release-it@19/schema/release-it.json",
    "github": {
      "release": false
    },
    "npm": {
      "publish": false
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": {
          "name": "angular"
        },
        "infile": "CHANGELOG.md"
      }
    }
  }
}
